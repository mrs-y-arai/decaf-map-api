{
  "name": "decaf-map-api",
  "version": "1.0.0",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "npx prisma generate && node dist/main.js",
    "dev": "nodemon --exec tsx src/main.ts",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier -c \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\" && prettier --write \"src/**/*.{ts,tsx}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prisma:pull": "prisma db pull",
    "prisma:generate": "prisma generate",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:seed": "tsx prisma/seed.ts",
    "test:db-migrate": "make test-services-db-migrate",
    "test": "make test-services-up && docker compose exec app-test sh -c 'npx vitest'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.3.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jwt-decode": "^4.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.38.4",
    "@eslint/js": "^9.21.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^22.10.10",
    "@vitest/coverage-v8": "^3.0.8",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.2",
    "prisma": "^6.3.0",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vitest": "^3.0.8"
  }
}
